package br.com.senaisp.richard.licao04;

public class Estudante {
	//Campos de instância ou Variáveis de instância
	public int id;
	public String nome;
	public String cpf;
	public double media;
	
	@Override
	public String toString() {
		return  "Id : " + id + 
				"\nnome : " + nome + 
				"\ncpf : " + cpf +
				"\nmedia : " + media;
	}
}
