package br.com.senaisp.richard.licao04;

public class Estudante {
	//Campos de instância ou Variáveis de instância
	private int id;
	private String nome;
	private String cpf;
	private double media;
	
	@Override
	public String toString() {
		return  "Id : " + id + 
				"\nnome : " + nome + 
				"\ncpf : " + cpf +
				"\nmedia : " + media;
	}
}
